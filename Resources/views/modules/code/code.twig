{% if toBeWrap %}
<div class="section">
    <div class="container">
        <div class="row">
            <div class="col s12 l9">
{% endif %}

        <div {% if identifier %} id="{{ identifier }}" class="scrollspy" {% endif %}>
            <h3>{{ header }}</h3>

            <p class="caption">
                {{ caption }}
